{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "baseUrl": "./",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "typeRoots": ["./node_modules/@types"],
    "types": ["node"],
    "strict": true,
    "outDir": "./dist",
    "skipLibCheck": true
  },
  "exclude": ["node_modules"]
}
